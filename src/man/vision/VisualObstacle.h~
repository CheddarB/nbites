#ifndef VISUAL_OBSTACLE_H
#define VISUAL_OBSTACLE_H


class VisualObstacle {


 public:

  // Constructor
  VisualObstacle();

  // Copy Constructor
  VisualObstacle(const VisualObstacle&);
  
  // Initialization
  void init();
  
  // Setters
  void onLeft(bool _isLeft) {onLeft = _isLeft;}
  void onRight(bool _isRight) {onRight = _isRight;}

  // Getters
  void getLeft() {return onLeft;}
  void getRight() {return onRight;}
  
 private:
  bool onLeft, onRight







#endif
